        android:minWidth="25px"
        android:minHeight="25px"
        android:background="@drawable/searchviewrounded"
        android:layout_marginTop="89.5dp"
        android:layout_marginLeft="263.0dp"
        android:layout_marginBottom="0.0dp"
        android:layout_marginRight="0.0dp" 
        />
    <TextView
        android:id="@+id/currentStoreLabelMain"
        android:text="Current Store"
        android:textColor="#001331"
        android:layout_width="170dp"
        android:layout_height="20dp"
        android:layout_marginTop="25.0dp"
        android:layout_marginLeft="34.0dp" 
        />
    <TextView
        android:id="@+id/dateRangeLabelMain"
        android:text="Date Range"
        android:textColor="#001331"
        android:layout_width="170dp"
        android:layout_height="20dp"
        android:layout_marginTop="90.0dp"
        android:layout_marginLeft="34.0dp" 
        />
    <TextView
        android:id="@+id/currentStoreMain"
        android:text="Select A Store"
        android:textColor="#777777"
        android:layout_width="170dp"
        android:layout_height="20dp"
        android:layout_marginTop="52.5dp"
        android:layout_marginLeft="34.0dp" 
        />
