<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff">
    <Button
        android:id="@+id/addButtonMain"
        android:text="Add"
        android:textSize="6pt"
        android:layout_width="90dp"
        android:layout_height="55dp"
        android:backgroundTint="#7fd178"
        android:textColor="@android:color/black"
        android:minWidth="25px"
        android:minHeight="25px"
        android:background="@drawable/searchviewrounded"
        android:layout_marginTop="153.5dp"
        android:layout_marginLeft="148.0dp"
        android:layout_marginBottom="0.0dp"
        android:layout_marginRight="0.0dp"
        />
    <Button
        android:id="@+id/couponsButtonMain"
        android:text="Coupon"
        android:textSize="5.73pt"
        android:layout_width="90dp"
        android:layout_height="55dp"
        android:backgroundTint="#9bb8f0"
        android:textColor="@android:color/black"
        android:minWidth="25px"
        android:minHeight="25px"
        android:background="@drawable/searchviewrounded"
        android:layout_marginTop="153.5dp"
        android:layout_marginLeft="34.0dp"
        android:layout_marginBottom="0.0dp"
        android:layout_marginRight="0.0dp" 
        />
    <Button
        android:id="@+id/deleteButtonMain"
        android:text="Delete"
        android:textSize="6pt"
        android:layout_width="90dp"
        android:layout_height="55dp"
        android:backgroundTint="#f6a796"
        android:textColor="@android:color/black"
        android:minWidth="25px"
        android:minHeight="25px"
        android:background="@drawable/searchviewrounded"
        android:layout_marginTop="153.5dp"
        android:layout_marginLeft="263.0dp"
        android:layout_marginBottom="0.0dp"
        android:layout_marginRight="0.0dp" 
        />
    <Button
        android:id="@+id/setStoreButtonMain"
        android:text="Set Store"
        android:textSize="6pt"
        android:layout_width="90dp"
        android:layout_height="55dp"
        android:backgroundTint="#9bb8f0"
        android:textColor="@android:color/black"
        android:minWidth="25px"
        android:minHeight="25px"
        android:background="@drawable/searchviewrounded"
        android:layout_marginTop="25.5dp"
        android:layout_marginLeft="263.0dp"
        android:layout_marginBottom="0.0dp"
        android:layout_marginRight="0.0dp" 
        />
    <Button
        android:id="@+id/setListButtonMain"
        android:text="Set List"
        android:textSize="6pt"
        android:layout_width="90dp"
        android:layout_height="55dp"
