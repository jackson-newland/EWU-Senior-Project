        android:id="@+id/dateRangeMain"
        android:text="Select A Date Range"
        android:textColor="#777777"
        android:layout_width="170dp"
        android:layout_height="20dp"
        android:layout_marginTop="117.5dp"
        android:layout_marginLeft="34.0dp" 
        />
