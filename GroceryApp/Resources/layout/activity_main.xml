        android:id="@+id/budgetMain"
        android:text="Budget: "
        android:textColor="#777777"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="224.0dp"
        android:layout_marginLeft="34.0dp"      
        />

    <TextView
        android:id="@+id/budgetMainValue"
        android:text="0"
        android:textColor="#777777"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="224.0dp"
        android:layout_marginLeft="85.0dp"      
        />

    <TextView
        android:id="@+id/remBudgetMain"
        android:text="Remaining Budget: "
        android:textColor="#777777"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="224.0dp"
        android:layout_marginLeft="200.0dp"      
        />

    <TextView
        android:id="@+id/remBudgetMainValue"
        android:text="0"
        android:textColor="#777777"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="224.0dp"
        android:layout_marginLeft="320.0dp"      
        />

    <ListView
        android:id="@+id/listViewMain"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:background="#f8fcfc"
        android:layout_marginTop="250.0dp"
        android:layout_marginLeft="0.0dp"
        android:layout_marginRight="0.0dp" 
        />
        
</RelativeLayout>